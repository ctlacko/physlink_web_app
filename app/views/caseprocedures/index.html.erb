<h1>Listing caseprocedures</h1>

<table>
  <tr>
    <th>Laterality</th>
    <th>Volume</th>
    <th>Patient case</th>
    <th>Procedure</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @caseprocedures.each do |caseprocedure| %>
  <tr>
    <td><%= caseprocedure.laterality %></td>
    <td><%= caseprocedure.volume %></td>
    <td><%= caseprocedure.patient_case_id %></td>
    <td><%= caseprocedure.procedure_id %></td>
    <td><%= link_to 'Show', caseprocedure %></td>
    <td><%= link_to 'Edit', edit_caseprocedure_path(caseprocedure) %></td>
    <td><%= link_to 'Destroy', caseprocedure, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Caseprocedure', new_caseprocedure_path %>
